<div class="container">
  <div class="row">
    <h4>Create Return</h4>
    <div class="col-auto">
      <%= simple_form_for @warranty do |f| %>
      <%= f.input :rma_no %>
      <%= f.input :remote, as: :boolean %>
      <%= f.input :stand, as: :boolean %>
      <%= f.input :damage, as: :boolean %>
      <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
